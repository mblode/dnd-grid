{
  "name": "@fingertip/react-grid-layout",
  "version": "2.0.19",
  "description": "A draggable and resizable grid layout with responsive breakpoints, for React.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "watch": "tsc --p ./tsconfig.build.json && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "release": "npm run build && npm version patch && npm publish",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hello-fingertip/react-grid-layout.git"
  },
  "keywords": [
    "react",
    "grid",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "fluid",
    "responsive"
  ],
  "author": "Matthew Blode <matt@fingertip.com> (https://fingertip.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hello-fingertip/react-grid-layout/issues"
  },
  "homepage": "https://github.com/hello-fingertip/react-grid-layout",
  "dependencies": {
    "@fingertip/prettier-config": "^1.0.2",
    "clsx": "^2.1.1",
    "fast-equals": "^5.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-draggable": "^4.4.6",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.27.0",
    "utility-types": "^3.11.0"
  },
  "peerDependencies": {
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.12",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@types/react-resizable": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "glob": "^11.0.0",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-resize-detector": "^11.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "overrides": {
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>test/util/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/test/spec/*.js"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "coverageThreshold": {
      "global": {
        "statements": 77,
        "branches": 72,
        "functions": 77,
        "lines": 78
      }
    }
  }
}
